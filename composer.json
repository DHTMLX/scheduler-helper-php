{
    "name": "dhtmlx/scheduler-helper",
    "description": "Helper for working with recurring events in dhtmlxScheduler.",
    "version": "1.0.0",
    "keywords": ["dhtmlx", "helper"],
    "license": "GNU",
    "type": "library",
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "DHTMLX_Scheduler\\Helper\\": "dhtmlx/scheduler-helper/"
        },
        "files": [
            "SchedulerHelper.php"
        ]
    },
    "min-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "vendor-dir": "/"
    }
}
